mixin s-faq()
	// start s-faq
	+b.s-faq.section#s-faq&attributes(attributes)
		.container
			mixin main-faq(i , t)
				+e.main-head.acc-head
					+icon('arrow-down')
					+e.IMG.img.res-i(src=`img/@2x/faq${i}.png` alt='')
					+e.main-title=t
				.acc-body
					block
			mixin in-faq
				+e.in-item
					+e.in-head.acc-head
						+icon('arrow-down')
						+e.in-title Lorem ipsum dolor sit amet,
